<!DOCTYPE HTML>
<html>
<head>
  <link rel="canonical" href="https://getbootstrap.com/docs/5.2/examples/starter-template/">

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    
  <link href="/docs/5.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
   <script type="text/javascript">
    const problems = <%- JSON.stringify(problems) %>
    console.log(problems);
    const verdict = <%- JSON.stringify(verdict) %>
    console.log(verdict);
    window.onload = function () {
	var chart = new CanvasJS.Chart("chartContainer1",
	{
		theme: "light2",
		title:{
			text: "Ratings of Problems Solved"
		},		
		data: [
		{       
			type: "pie",
			showInLegend: true,
			toolTipContent: "#percent %",
			yValueFormatString: "#,##,,.##",
			legendText: "{indexLabel}",
			dataPoints: problems
		}
		]
	});
    chart.render(); 

	var chart = new CanvasJS.Chart("chartContainer2",
	{
		theme: "light2",
		title:{
			text: "Problems Verdict"
		},		
		data: [
		{       
			type: "pie",
			showInLegend: true,
			toolTipContent: "#percent %",
			yValueFormatString: "#,##,,.##",
			legendText: "{indexLabel}",
			dataPoints: verdict
		}
		]
	});
	chart.render(); 
   }
   </script>
   <style>
    body {
      background-color:#142850;
      color:#00A8CC;
    } 
    .home-btn {
      margin-top: 40px;
    } 
   </style>
<script type="text/javascript" src="https://canvasjs.com/assets/script/canvasjs.min.js"></script></head>
<body>
<center><h1>RATINGS OF PROBLEMS SOLVED</h1></center>
<div id="chartContainer1" style="height: 300px; width: 100%;"></div>
<center><h1>PROBLEMS VERDICT</h1></center>
<div id="chartContainer2" style="height: 300px; width: 100%;"></div>
<center>
    <div class="mb-5 home-btn">
      <form action="/" method="GET">
      <button class="btn btn-outline-primary btn-lg px-4 ">HOME</button>
      </form>
    </div>
</center>
</body>
 </html>